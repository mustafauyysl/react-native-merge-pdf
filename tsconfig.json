{
    "compilerOptions": {
      "target": "ES2019",
      "module": "ESNext",
      "lib": ["ESNext"],
      "jsx": "react",
      "declaration": true,
      "declarationMap": true,
      "outDir": "lib/typescript",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "example", "lib"]
  }
  